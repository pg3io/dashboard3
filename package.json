    "bootstrap-icons": "^1.2.1",
    "bootstrap-vue": "^2.17.3",
    "chart.js": "^3.5.1",
    "core-js": "^3.6.5",
    "eslint-plugin-graphql": "^4.0.0",
    "graphql": "^15.5.1",
    "highcharts": "^9.2.2",
    "jquery": "^3.5.1",
    "js-md5": "^0.7.3",
    "js-yaml": "^3.14.0",
