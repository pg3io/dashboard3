    "vue": "^2.6.11",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-axios": "^3.2.0",
    "vue-chartkick": "^0.6.1",
    "vue-cli": "^2.9.6",
    "vue-gravatar": "^1.4.1",
    "vue-pdf": "^4.2.0",
